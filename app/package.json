{
  "name": "@fight-picks/app",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "androidProd": "eas build --profile production --platform android",
    "iosProd": "eas build --profile production --platform ios",
    "androidDev": "eas build --profile development --platform android --local",
    "iosDev": "eas build --profile development --platform ios --local",
    "iosSim": "eas build --profile development-sim --platform ios --local",
    "iosSimProd": "eas build --profile production-sim --platform ios --local",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "eslint . --cache --fix --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "genScreen": "hygen screen new",
    "firebaseEmulators": "firebase emulators:start --project fight-picks",
    "deployFirestoreRules": "firebase deploy --only firestore:rules"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@fight-picks/models": "1.0.0",
    "@invertase/react-native-apple-authentication": "^2.2.2",
    "@react-native-firebase/app": "^17.1.0",
    "@react-native-firebase/auth": "^17.1.0",
    "@react-native-firebase/firestore": "^17.1.0",
    "@react-native-google-signin/google-signin": "^9.0.2",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@reduxjs/toolkit": "^1.9.2",
    "core-js": "^3.28.0",
    "cross-env": "5.0.5",
    "date-fns": "^2.29.3",
    "deepmerge": "^4.2.2",
    "expo": "~47.0.12",
    "expo-asset": "~8.7.0",
    "expo-build-properties": "~0.4.1",
    "expo-constants": "~14.0.2",
    "expo-dev-client": "~2.0.1",
    "expo-font": "~11.0.1",
    "expo-linking": "~3.3.0",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "expo-system-ui": "~2.0.1",
    "expo-web-browser": "~12.0.0",
    "nanoid": "^4.0.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-get-random-values": "~1.8.0",
    "react-native-paper": "^5.1.3",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-web": "~0.18.9",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@faker-js/faker": "^7.6.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "hygen": "^6.2.11",
    "jest": "^26.6.3",
    "jest-expo": "~47.0.1",
    "react-test-renderer": "18.1.0"
  },
  "private": true
}
